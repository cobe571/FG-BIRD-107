<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>speed.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.8 </red>
      <green> 0.7 </green>
      <blue>  0.5 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  200 </ind><dep>  25 </dep></entry>
      <entry><ind>  400 </ind><dep>  77 </dep></entry>
      <entry><ind>  600 </ind><dep> 129 </dep></entry>
      <entry><ind>  800 </ind><dep> 180 </dep></entry>
      <entry><ind> 1000 </ind><dep> 232 </dep></entry>
      <entry><ind> 1200 </ind><dep> 283 </dep></entry>
      <entry><ind> 1400 </ind><dep> 335 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


